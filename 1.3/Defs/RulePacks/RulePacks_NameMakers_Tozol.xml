<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RulePackDef>
	<defName>NamerPersonTozol</defName>
	<rulePack>
		<rulesStrings>
			<li>name->[TozolFirst] [TozolLast]</li>
      		<li>name->[TozolFirst] '[TozolNick]' [TozolLast]</li>
		</rulesStrings>
		<rulesRaw>
			<li Class="Rule_File">
				<keyword>TozolFirst</keyword>
				<path>TozolNames/TozolFirst</path>
			</li>
			<li Class="Rule_File">
				<keyword>TozolLast</keyword>
				<path>TozolNames/TozolLast</path>
			</li>
			<li Class="Rule_File">
				<keyword>TozolNick</keyword>
				<path>TozolNames/TozolNick</path>
			</li>
		</rulesRaw>
	</rulePack>
</RulePackDef>
</Defs>